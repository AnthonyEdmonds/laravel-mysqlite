{
    "name": "anthonyedmonds/laravel-mysqlite",
    "description": "Generate MySQL and Sqlite3 expressions on the fly based on the active database",
    "keywords": ["laravel", "mysql", "sqlite", "database"],
    "type": "library",
    "homepage": "https://github.com/AnthonyEdmonds/laravel-mysqlite",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelMySqlite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelMySqlite\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Anthony Edmonds",
            "email": "contact@anthonyedmonds.me",
            "homepage": "https://anthonyedmonds.me",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11"
    },
    "require-dev": {
        "orchestra/testbench": "^9",
        "phpstan/phpstan": "^1",
        "phpunit/phpunit": "^11",
        "laravel/pint": "^1"
    }
}
